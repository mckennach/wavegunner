    "dist" 
  ],
  "exports": { 
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "react",
    "component",
    "npm"
  ],
  "repository": {
    "type": "git",
    "url": "git+git+https://github.com/mckennach/wavegunner.git"
  },

